{
  "name": "canino",
  "version": "1.0.0",
  "authors": [
    "Ignacio Cruz <igmoweb@gmail.com>"
  ],
  "description": "Canino Magazine Theme",
  "main": "gulpfile.js",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "bower": "^1.8.2",
    "compress-images": "^1.4.6",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "gulp": "^3.9.0",
    "gulp-clean": "^0.3.2",
    "gulp-image": "^4.4.1",
    "gulp-sass": "^2.1.0",
    "gulp-uglify": "^2.0.0",
    "gulp-uglifycss": "^1.0.6",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "require-dir": "^0.3.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "system-bell-webpack-plugin": "^1.0.0",
    "webpack": "^2.7.0",
    "webpack-merge": "^4.1.0",
    "whatwg-fetch": "^2.0.1",
    "yargs": "^6.6.0"
  },
  "scripts": {
    "postinstall": "bower install && npm run build",
    "watch": "webpack --watch",
    "build": "webpack -p --env.production=1 && gulp build",
    "phpcs": "./bin/composer.sh run-script phpcs",
    "phpcs:fix": "./bin/composer.sh run-script phpcs:fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zurb/foundation-sites-template.git"
  },
  "bugs": {
    "url": "https://github.com/zurb/foundation-sites/issues",
    "email": "foundation@zurb.com"
  },
  "author": "Ignacio Cruz <igmoweb@gmail.com>",
  "license": "MIT",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "relative": true,
    "linters": {
      "**/*.php": [
        "./bin/composer.sh run-script phpcs:fix",
        "git add",
        "./bin/composer.sh run-script phpcs"
      ]
    }
  }
}
